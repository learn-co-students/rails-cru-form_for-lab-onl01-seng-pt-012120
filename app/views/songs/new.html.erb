<h1>My Song</h1>
<%= form_for(@song) do |f| %>
  <label>My song name:</label><br>
  <%= f.text_field :name %><br>
  <label>My song artist ID:</label><br>
  <%= f.text_field :artist_id %><br>
  <label>My song genre ID:</label><br>
  <%= f.text_field :genre_id %><br>
  <%= f.submit "Create Song" %>
<% end %>